{
  "name": "@flair/starter-boilerplate",
  "version": "1.0.0",
  "description": "Indexing boilerplate components and scripts",
  "main": "manifest.yml",
  "repository": "git@github.com:flair-sdk/starter-boilerplate.git",
  "author": "gm@flair.dev",
  "license": "MIT",
  "private": true,
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "generate": "find ./ -type f -name '*.mustache' -exec sh -c 'mustache ./config.json \"$1\" > \"${1%.mustache}\"' _ {} \\;",
    "deploy": "pnpm run generate && flair deploy"
  },
  "devDependencies": {
    "@types/node": "^20.3.2",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "flair-cli": "^1.18.0",
    "mustache": "^4.2.0",
    "prettier": "^2.8.8",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@ethersproject/bignumber": "^5.7.0",
    "flair-sdk": "^1.1.12"
  }
}
